package µÚ¶þ³¡;

import java.util.Scanner;

public class D
{

	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		while(s.hasNextInt())
		{
			int sum=0;
			int n=s.nextInt();
			while(n-->0)
			{
				int temp=s.nextInt();
				if(temp<60)
				{
					sum+=40;
				}
			}
			System.out.println(sum);
		}
	}

}
