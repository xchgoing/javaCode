package acm2018;

import java.util.Arrays;
import java.util.Scanner;

public class zoj3944
{

	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		while(s.hasNextInt())
		{
			int n=s.nextInt();
			for(int a=0;a<n;a++)
			{
				int q=s.nextInt();
				int w=s.nextInt();
				String[][] arr=new String[q+4][w+4];
				for(int b=0;b<q+4;b++)
				{
					for(int c=0;c<w+4;c++)
					{
						if(b<2||c<2||b>arr.length-3||c>arr[0].length-3)
						{
							arr[b][c]="*";
							continue;
						}
						arr[b][c]=s.next();
					}
//					System.out.println("ok");
				}
				for(int b=0;b<q+4;b++)
				{
					for(int c=0;c<w+4;c++)
					{
						System.out.print(arr[b][w]);
					}
					System.out.println();
				}
//				for(int b=0;b<q;b++)
//				{
//					for(int c=0;c<w;a++)
//					{
//						
//					}
//				}
			}
		}
	}

}
