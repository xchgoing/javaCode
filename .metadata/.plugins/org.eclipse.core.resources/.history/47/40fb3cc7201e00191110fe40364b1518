package µÚÒ»³¡0122;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashSet;
import java.util.Scanner;

public class J
{

	public static void main(String[] args)
	{
		Scanner s=new Scanner (System.in);
		while(s.hasNextInt())
		{
			int n=s.nextInt();
			int m=s.nextInt();
			ArrayList<Integer>[] sets=new ArrayList[n+1]; 
			int[] arr=new int[n+1];
			Arrays.fill(arr,0);
			Arrays.fill(sets,new ArrayList<Integer>());
			while(m-->0)
			{
				int opt=s.nextInt();
				int l=s.nextInt();
				int r=s.nextInt();
				int x=s.nextInt();
				if(opt==1)
				{
					for(int a=l;a<=r;a++)
					{
						sets[a].add(x);
						arr[a]^=x;
					}
				}else if(opt==2){
					for(int a=l;a<=r;a++)
					{
						if(sets[a].remove(new Integer(x)))
						arr[a]^=x;
					}
				}else
				{
					int result=0;
					for(int a=l;a<=r;a++)
					{
						result^=arr[a];
					}
					System.out.println(result);
				}
			}
		}
	}

}
